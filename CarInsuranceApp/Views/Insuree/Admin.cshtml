@model IEnumerable<CarInsuranceApp.Models.Insuree>

@{ ViewBag.Title = "Admin"; }

<h2>Admin View</h2>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Quote</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var insuree in Model)
        {
<tr>
    <td>@insuree.FirstName</td>
    <td>@insuree.LastName</td>
    <td>@insuree.EmailAddress</td>
    <td>@String.Format("{0:C}", insuree.Quote)</td>
</tr>}
    </tbody>
</table>
